    <select id="selectProjectById" parameterType="Long" resultType="org.funding.project.vo.ProjectVO">
        SELECT
            project_id,
            title,
            promotion,
            project_type,
            progress,
            create_at,
            deadline,
            user_id
        FROM project
        WHERE project_id = #{projectId}
